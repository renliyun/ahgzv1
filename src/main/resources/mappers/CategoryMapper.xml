<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ahgz.mapper.CategoryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ahgz.entity.Category">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="created_time" property="createdTime" />
        <result column="created_name" property="createdName" />
        <result column="updated_time" property="updatedTime" />
        <result column="updated_name" property="updatedName" />
        <result column="mark" property="mark" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, created_time, created_name, updated_time, updated_name, mark
    </sql>

</mapper>
